##contracts data pull
def conracts_check():
    item_1 = requests.get('https://api.spacetraders.io/v2/my/contracts/', headers=headers)
    print("Code:",item_1.status_code,"\n",item_1.text)
##agent data pull
def agent_info():
    response = requests.get('https://api.spacetraders.io/v2/my/agent', headers=headers)
    print("Code:",response.status_code)
    print(response.text)
    agent_data = tk.Label(text=response.text)
    agent_data.pack()